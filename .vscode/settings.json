{
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/node_modules": true,
    "**/Thumbs.db": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.inlayHints.enabled": "onUnlessPressed",
  "twoslashQueries.enabled": true,
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
